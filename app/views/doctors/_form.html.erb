<%= render 'layouts/navbar' %>
<h1><%= @dr.id ? 'Edit Doctor' : 'Create Doctor' %></h1>
<hr />
<%= form_for @dr do |f| %>
  <%= f.label :first_name %>
  <%= f.text_field :first_name, required: true, autofocus: true %>

  <%= f.label :last_name %>
  <%= f.text_field :last_name, required: true%>

  <%= f.submit %>
<% end %>